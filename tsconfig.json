{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["./components/*"],
      "screens/*": ["./screens/*"],
      "assets/*": ["./assets/*"],
      "hooks/*": ["./hooks/*"],
      "utils/*": ["./utils/*"],
      "types/*": ["./types/*"],
      "mobile-types/*": ["./mobile-types/*"],
      "constants/*": ["./constants/*"],
      "themes/*": ["./themes/*"],
      "graphql/*": ["./graphql/*"],
      "contexts/*": ["./contexts/*"],
      "server/*": ["./server/*"]
    },
    "typeRoots": ["./types", "./mobile-types", "./node_modules/@types"]
  },
  "include": ["**/*.ts", "**/*.tsx", "types/**/*.d.ts", "mobile-types/**/*.d.ts"],
  "exclude": ["node_modules"]
}
